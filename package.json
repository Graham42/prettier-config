{
  "name": "@graham42/prettier-config",
  "version": "2.1.0",
  "engines": {
    "node": ">=12"
  },
  "description": "Automated prettier config setup",
  "main": "src/index.js",
  "bin": "src/setup.js",
  "files": [
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Graham42/prettier-config.git"
  },
  "scripts": {
    "check:types": "tsc -p jsconfig.json",
    "test": "bash ./setup.test.sh",
    "lint:format": "prettier --list-different '**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}' ",
    "fix:format": "prettier --write '**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}' "
  },
  "keywords": [
    "prettier",
    "project-setup",
    "auto-formatter"
  ],
  "author": "Graham McGregor",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^12.20.37",
    "@types/prettier": "^2.4.2",
    "prettier": "^2.5.1",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "prettier": "^2.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2"
  }
}
