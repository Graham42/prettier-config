{
  "name": "@graham42/prettier-config",
  "version": "1.3.2",
  "engines": {
    "node": ">=8"
  },
  "description": "Opinionated prettier config",
  "main": "index.js",
  "bin": "./setup.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Graham42/prettier-config.git"
  },
  "scripts": {
    "lint": "tsc",
    "test": "bash ./setup.test.sh",
    "lint:format": "prettier --list-different '**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}' ",
    "fix:format": "prettier --write '**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}' "
  },
  "keywords": [
    "prettier",
    "auto-formatter"
  ],
  "author": "Graham McGregor",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^8.10.29",
    "@types/prettier": "^1.13.2",
    "prettier": "^2.1.2",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {
    "prettier": "^2.0.0"
  },
  "dependencies": {
    "meow": "^7.1.1"
  }
}
